#include <iostream>
using namespace std;

int volume(){
    float PI=3.14159265358979, height, radius, volume;
    cout<<"***** Hello Welcome to Cylinder Volume finder *****"<<endl;
    cout<<"Enter Radius of Cylinder in 'cm': "<<endl;
    cin>>radius;
    cout<<"Enter Height of Cylinder in 'cm': "<<endl;
    cin>>height;
    volume=PI*radius*radius*height;
    cout<<"Your Volume is: "<<volume<<endl;
    
}
int main() {
    volume();
    return 0;
}
